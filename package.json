{
  "name": "react-gcal-monorepo",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev:site": "npm run dev --workspace=site",
    "dev": "vite",
    "dev:lib": "npm run dev --workspace=react-gcal",
    "build": "npm run build --workspace=site",
    "build:dev": "npm run build:dev --workspace=site",
    "build:lib": "npm run build --workspace=react-gcal",
    "preview": "npm run preview --workspace=site",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.83.0",
    "lucide-react": "^0.562.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "lovable-tagger": "^1.1.13",
    "postcss": "^8.5.6",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "vite": "^5.4.19"
  }
}
