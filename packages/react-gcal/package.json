{
  "name": "react-gcal",
  "version": "0.1.3",
  "type": "module",
  "description": "A beautiful Google Calendar-inspired component library for React and Next.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && sass src/styles/styles.scss dist/styles.css --no-source-map",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "build:types": "tsc --project tsconfig.types.json"
  },
  "keywords": [
    "react",
    "nextjs",
    "calendar",
    "google-calendar",
    "gcal",
    "planner",
    "schedule",
    "events",
    "ui",
    "component-library"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.462.0",
    "react-day-picker": "^8.10.1"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "esbuild-sass-plugin": "^3.6.0",
    "sass": "^1.97.2",
    "standard-version": "^9.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3"
  }
}
